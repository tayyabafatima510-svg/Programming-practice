#include<iostream>
using namespace std;
int main()
{
	for(int i=1;i<=7;i++)
	{
	for(int j=1;j<=9;j++)
	if ((j==5&&i==1)||(i==2&&j==6)||(i==2&&j==4)||(i==3&&j==3)||(i==4&&j==2)||(i==5&&j==1)||(i==3&&j==4)||(i==3&&j==5)||(i==3&&j==6)||(i==3&&j==7)||(i==4&&j==8)||(i==5&&j==9))
	{
		cout<<"*";
	}
	else
	{
	cout<<" ";
}
cout<<endl;
}
	return 0;
}
