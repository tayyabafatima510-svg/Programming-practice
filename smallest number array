#include<iostream>
using namespace std;
int main()
{
	int numbers[5];
	int smallest;
cout<<"enter five numbers";
{
for(int i=0;i<5;i++)
{
cin>>numbers[i];
}
smallest=numbers[0];
for(int i=0;i<5;i++)
if(numbers[i]<smallest)
{
smallest=numbers[i];
}
}
cout<<"smallest number is"<<smallest;
return 0;
}

